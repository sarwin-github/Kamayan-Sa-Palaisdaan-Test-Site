<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <link rel="stylesheet" href="/css/bootstrap.min.css">
  <link rel="stylesheet" href="/css/bootstrap.css">
  <link rel="stylesheet" href="/css/font-awesome.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
  <script src="/js/bootstrap.js"></script>
  <script src="/js/bootstrap.min.js"></script>
  <style type="text/css">
   body{
    background-color: pink;
  }
  .navbar-brand{
    padding-bottom: 50px;
  }

  .background:before {
    content:'';
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background:url('public/images/nav-background.jpg');
    background-size: 100%;
    background-repeat: repeat-y;
    z-index:-1;
  }

  .shadow-box{
    -webkit-box-shadow: 0px 1px 5px 0px rgba(0,0,0,0.60);
    -moz-box-shadow: 0px 1px 5px 0px rgba(0,0,0,0.60);
    box-shadow: 0px 1px 5px 0px rgba(0,0,0,0.60);
  }

  .menu-style{
    border-bottom:1px solid;
    padding: 3px;"
  }
</style>
</head>
<body style="background:url('public/images/nav-background.jpg')">
  <div class="container" style="background-color:rgba(255, 255, 255, 0.9)">
    <div class="row">
      <% include templates/navbar.ejs%>
      <% include templates/navbar-second.ejs%>
      <% include templates/banner-carousel.ejs%>
      <div>
        <div class="col-md-12">
          <div class="col-sm-12">
            <div class="col-sm-4">
              <img src="/public/images/kamayanLogo2.png" class="img-responsive" />
              <ul class="list-group">
                <li  class="list-group-item"><img src="/public/images/foods.jpg" class="img-thumbnail img-responsive" width="340" /></li>
                <li  class="list-group-item"><img src="/public/images/floating.jpg" class="img-thumbnail img-responsive" width="340" /></li>
                <li  class="list-group-item"><img src="/public/images/southeast-asia-philippines-quezon-tayabas-kamayan-sa-palaisdaan-5.jpg" class="img-thumbnail img-responsive" width="340" /></li>
                <li  class="list-group-item"><img src="/public/images/palaisdaan2.jpg" class="img-thumbnail img-responsive" width="340" /></li>
                <li class="list-group-item">
                  <div class="text-center">
                  <legend>Follow us on</legend>
                    <span class="fa-stack fa-lg">
                      <a>
                        <i class="fa fa-square-o fa-stack-2x"></i>
                        <i class="fa fa-twitter fa-stack-1x"></i>
                      </a>
                    </span>
                    <span class="fa-stack fa-lg">
                      <a>
                        <i class="fa fa-square-o fa-stack-2x"></i>
                        <i class="fa fa-facebook fa-stack-1x"></i>
                      </a>
                    </span>
                    <span class="fa-stack fa-lg">
                      <a>
                        <i class="fa fa-square-o fa-stack-2x"></i>
                        <i class="fa fa-yahoo fa-stack-1x"></i>
                      </a>
                    </span>
                    <span class="fa-stack fa-lg">
                      <a>
                        <i class="fa fa-square-o fa-stack-2x"></i>
                        <i class="fa fa-google fa-stack-1x"></i>
                      </a>
                    </span>
                    <span class="fa-stack fa-lg">
                      <a>
                        <i class="fa fa-square-o fa-stack-2x"></i>
                        <i class="fa fa-linkedin fa-stack-1x"></i>
                      </a>
                    </span>
                  </div>
                </li>
              </ul>

            </div>
            <% include templates/description.ejs%>
          </div>
        </div>   
      </div> 
    </div>
    <br>
    <div id="map" style="width:100%; height: 300px"></div>

    <script>
      function initMap() {
        var uluru = {lat: 14.0616, lng: 121.5721};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 11,
          center: uluru
        });
        var marker = new google.maps.Marker({
          position: uluru,
          map: map
        });
      }
    </script>
    <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDmYO1nfXEKwBDkGBSOr-bpqyKvOFRR2N4&callback=initMap">
    </script>

  </div>

</body>
</html>
